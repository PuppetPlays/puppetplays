{
  "name": "puppetplays-web",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "dev": "next-translate && NODE_ENV=development node server.js",
    "proxy-ssl": "npx local-ssl-proxy --source 7001 --target 7000",
    "start": "next start",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "lint": "eslint .",
    "build": "next-translate && next build",
    "deploy": "npm run test:ci && npm run build"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "./"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "next": "10.0.1",
    "next-translate": "^0.20.2",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.0",
    "react": "17.0.1",
    "react-accessible-dropdown-menu-hook": "^2.2.2",
    "react-dom": "17.0.1",
    "react-modal": "^3.13.1",
    "react-paginate": "^6.5.0",
    "react-select": "^4.3.0",
    "react-slider": "^1.1.4",
    "swr": "^0.3.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "babel-jest": "^26.6.3",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "http-proxy-middleware": "^1.0.6",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "sass": "^1.29.0"
  }
}
